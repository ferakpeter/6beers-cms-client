{
  "name": "6beers-client-app",
  "private": true,
  "version": "0.0.0",
  "description": "Client for the 6beers web application",
  "main": "src/index.re",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:bsb": "bsb -make-world",
    "build:webpack": "webpack -p",
    "clean": "npm-run-all clean:*",
    "clean:bsb": "bsb -clean-world",
    "clean:project": "rimraf public/main.js lib .merlin 'src/**/*.js'",
    "start": "node scripts/startDev.js",
    "start:parallel": "npm-run-all --parallel start:bsb start:webpack",
    "start:bsb": "npm run build:bsb -- -w",
    "start:webpack": "node scripts/webpackDevServer.js",
    "jest": "jest",
    "test": "npm-run-all build:bsb jest"
  },
  "keywords": [
    "reason",
    "react",
    "6beers"
  ],
  "author": "Peter Ferak",
  "license": "MIT",
  "dependencies": {
    "bs-fetch": "^0.2.1",
    "@glennsl/bs-json": "^1.1.2",
    "figlet": "^1.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "reason-react": "^0.3.4",
    "react-flipcard": "^0.2.1",
    "react-slick": "^0.18.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "bs-jest": "^0.1.0",
    "bs-platform": "^2.2.3",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "pushstate-server": "^3.0.0",
    "reason-cli": "https://github.com/reasonml/reason-cli.git#beta-v-1.13.6-bin-darwin",
    "rimraf": "^2.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "jest": {
    "roots": [
      "src"
    ]
  }
}
